{
  "name": "@ecadlabs/auth-webapp",
  "description": "A reference implementation web application for the ecad auth daemon. Includes a CRUD module to manage users and roles.",
  "license": "UNLICENSED",
  "version": "0.14.2-beta18",
  "scripts": {
    "ng": "ng",
    "start": "ng build --prod ecad-angular-auth && ng serve --host 0.0.0.0",
    "build": "ng build ecad-angular-auth && ng build",
    "build-prod": "ng build --prod ecad-angular-auth && ng build",
    "test": "ng test && ng test ecad-angular-auth",
    "test-webapp": "ng test webapp --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test-ecad-angular-auth": "ng test ecad-angular-auth --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test-ci": "npm run test-webapp && npm run test-ecad-angular-auth",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc:serve": "./node_modules/.bin/compodoc . -s",
    "compodoc": "./node_modules/.bin/compodoc -p projects/ecad-angular-auth/tsconfig.lib.json --theme vagrant"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.3",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^7.0.3",
    "@angular/compiler": "^7.0.3",
    "@angular/core": "^7.0.3",
    "@angular/forms": "^7.0.3",
    "@angular/http": "^7.0.3",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^7.0.3",
    "@angular/platform-browser-dynamic": "^7.0.3",
    "@angular/router": "^7.0.3",
    "@auth0/angular-jwt": "^2.0.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.5",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "^7.0.3",
    "@angular/language-service": "^7.0.3",
    "@compodoc/compodoc": "^1.1.6",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tsickle": "^0.33.1",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6"
  }
}
